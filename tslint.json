{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {
    "no-console": false,
    "quotemark": false,
    "ordered-imports": false,
    "object-literal-key-quotes": false,
    "object-literal-sort-keys": false
  },
  "rules": {
    "no-console": false,
    "quotemark": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "max-line-length": [true, { "limit": 120, "ignore-pattern": "['\"].*['\"]" }]
  },
  "rulesDirectory": []
}
